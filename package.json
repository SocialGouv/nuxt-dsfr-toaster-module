{
  "name": "@socialgouv/dsfr-toaster-nuxt-module",
  "version": "1.0.3",
  "description": "Module Nuxt Dsfr Toaster",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build --stub && nuxt-module-build prepare ",
    "dev": "nuxi dev playground --port 3080",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.11.2",
    "@gouvminint/vue-dsfr": "~5.13.0",
    "@nuxt/kit": "^3.11.2",
    "defu": "~6.1.4",
    "oh-vue-icons": "~1.0.0-rc3"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.1.5",
    "@nuxt/eslint-config": "^0.3.6",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.12.0",
    "@socialgouv/eslint-config-recommended": "^1.131.0",
    "@types/node": "^20.12.7",
    "changelogen": "^0.5.5",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nuxt": "^3.11.2",
    "prettier": "^3.2.5",
    "sass": "~1.72.0",
    "sass-loader": "~14.1.1",
    "typescript": "~5.4.3",
    "vitest": "^1.4.0"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "lint-staged": {
    "src/**/*.{js,vue,ts}": "npx eslint --cache --fix"
  }
}
